{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-unused-vars": [0],
      "import/no-cycle": [0],
      "react/jsx-filename-extension": [0],
      "react/prefer-stateless-function": [0],
      "react/prop-types": [0],
      "max-len": [0],
      "react/button-has-type": [0],
      "no-underscore-dangle": [0],
      "max-classes-per-file": [0],
      "class-methods-use-this": [0],
      "react/destructuring-assignment": [0],
      "quotes": ["error", "double"],
      "no-param-reassign": [0]
    }
}
